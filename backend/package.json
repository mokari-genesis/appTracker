{
  "name": "keeply-core",
  "version": "1.0.0",
  "main": "handler.js",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^19.0.7",
    "serverless-offline": "^12.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "globals": "^16.2.0",
    "node-pg-migrate": "^7.0.0",
    "pg": "^8.16.3",
    "prettier": "^3.5.3"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate": "dotenv -e .env -- node-pg-migrate up",
    "migrate:down": "dotenv -e .env -- node-pg-migrate down",
    "migrate:create": "dotenv -e .env -- node-pg-migrate create -m migrations"
  },
  "node-pg-migrate": {
    "migrationFolder": "migrations"
  }
}
